{
	"name": "reacting-squirrel",
	"version": "0.16.0",
	"description": "React and Express framework for creating web apps.",
	"main": "index.js",
	"types": "./app/index.d.ts",
	"scripts": {
		"test": "./node_modules/.bin/mocha __tests__/cases/server __tests__/cases/app --recursive --require @babel/register --require ignore-styles --exit",
		"test_app_compile": "./node_modules/.bin/babel ./__tests__app__/dev/src --out-dir ./__tests__app__/dev/dist && node -r source-map-support/register ./__tests__app__/dev/dist/index.js",
		"test_app": "npm run build_es6 && npm run test_app_compile",
		"posttest": "rm -rf ./__tests__/app/~rs && rm -rf ./__tests__/app/pages && rm -rf ./__tests__/app/components && rm -rf ./__tests__/public",
		"create_index": "run-script-os",
		"create_index:win32": "echo module.exports = require('./app'); > index.js",
		"create_index:linux:darwin": "echo \"module.exports = require('./app');\" > index.js",
		"build_es6": "./node_modules/.bin/babel ./src --out-dir ./ --copy-files && npm run create_index",
		"clear": "rm -rf ./app && rm -rf ./server && rm -rf ./cli && rm -f ./index.js",
		"prepublish": "npm run build_es6",
		"postpublish": "npm run clear",
		"docs_generate": "./node_modules/.bin/typedoc --out docs ./src --includeDeclarations --excludeExternals",
		"docs": "rm -rf ./docs && npm run docs_generate && touch ./docs/.nojekyll",
		"preversion": "git branch %npm_package_version% && git checkout %npm_package_version%"
	},
	"author": "Lukáš Mačuda <lukas.macuda@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.8.3",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/plugin-transform-async-to-generator": "^7.8.3",
		"@babel/polyfill": "^7.8.3",
		"@babel/preset-env": "^7.8.3",
		"@babel/preset-react": "^7.8.3",
		"@types/express": "^4.17.2",
		"@types/node": "^10.17.13",
		"@types/react": "^16.9.17",
		"@types/socket.io": "^2.1.4",
		"args-parser": "^1.1.0",
		"async": "^2.6.3",
		"autoprefixer": "^9.7.4",
		"babel-loader": "^8.0.6",
		"combine-styles-processor": "^1.3.2",
		"compression": "^1.7.4",
		"cookie": "^0.4.0",
		"cookie-parser": "^1.4.4",
		"cookie-signature": "^1.1.0",
		"css-loader": "^3.4.2",
		"express": "^4.17.1",
		"extra-watch-webpack-plugin": "^1.0.3",
		"html-to-react": "^1.4.2",
		"http-smart-error": "^3.0.0",
		"loader-utils": "^1.2.3",
		"lodash": "^4.17.15",
		"md5": "^2.2.1",
		"mkdirp": "^0.5.1",
		"msgpack-lite": "^0.1.26",
		"node-sass": "^4.12.0",
		"postcss": "^7.0.26",
		"postcss-loader": "^3.0.0",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"route-parser": "0.0.5",
		"runtime-type": "^3.3.0",
		"sass-loader": "^8.0.2",
		"smart-error": "^3.0.0",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.0",
		"style-loader": "^1.1.3",
		"texting-squirrel": "^1.0.0",
		"ts-loader": "^6.2.0",
		"typescript": "^3.7.5",
		"uniqid": "^5.2.0",
		"universal-cookie": "^4.0.3",
		"webpack": "^4.41.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.3",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/plugin-transform-react-jsx": "^7.8.3",
		"@babel/register": "^7.8.3",
		"babel-eslint": "^10.0.3",
		"bootstrap": "^4.4.1",
		"chai": "^4.2.0",
		"chai-enzyme": "^1.0.0-beta.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.18.0",
		"ignore-styles": "^5.0.1",
		"jsdom": "^15.2.1",
		"mocha": "^6.2.2",
		"react-gtm-module": "^2.0.8",
		"request": "^2.88.0",
		"run-script-os-fix": "^1.0.4",
		"source-map-support": "^0.5.16",
		"typedoc": "^0.15.8"
	},
	"engines": {
		"node": ">=8.9"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zabkwak/reacting-squirrel.git"
	},
	"bugs": {
		"url": "https://github.com/zabkwak/reacting-squirrel/issues"
	},
	"homepage": "https://zabkwak.github.io/reacting-squirrel",
	"keywords": [
		"react",
		"express",
		"socket.io",
		"framework"
	],
	"readme": "README.md",
	"bin": {
		"rs-start-server": "./cli/server.js"
	}
}
