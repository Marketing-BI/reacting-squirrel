{
  "name": "reacting-squirrel",
  "version": "0.8.3",
  "description": "React and Express framework for creating web apps.",
  "main": "index.js",
  "types": "./__types__/index.d.ts",
  "scripts": {
    "test": "mocha __tests__/cases/server __tests__/cases/app --recursive --compilers js:@babel/register --require ignore-styles --timeout 20000",
    "test_app_compile": "./node_modules/.bin/babel ./__tests__/dev/src --out-dir ./__tests__/dev/dist && node ./__tests__/dev/dist/index.js",
    "test_app": "npm run build_es6 && npm run test_app_compile",
    "posttest": "rm -rf ./__tests__/app/~rs && rm -rf ./__tests__/public",
    "create_index": "run-script-os",
    "create_index:win32": "echo module.exports = require('./app'); > index.js",
    "create_index:linux:darwin": "echo \"module.exports = require('./app');\" > index.js",
    "build_es6": "./node_modules/.bin/babel ./src --out-dir ./ --copy-files && npm run create_index",
    "clear": "rm -rf ./app && rm -rf ./server && rm -f ./index.js",
    "prepublish": "npm run build_es6",
    "postpublish": "npm run clear",
    "docs_server": "./node_modules/.bin/jsdoc ./src/server -r -d ./docs/server -c ./jsdoc.json",
    "docs_app": "./node_modules/.bin/jsdoc ./src/app -r -d ./docs/app -c ./jsdoc.json",
    "docs_generate": "./node_modules/.bin/jsdoc -c ./jsdoc.json",
    "docs": "rm -rf docs && npm run docs_generate",
    "md_server": "./node_modules/.bin/jsdoc2md --files ./src/server/**/*.js > ./markdown/server.md",
    "md_app": "./node_modules/.bin/jsdoc2md --files ./src/app/**/*.js > ./markdown/app.md",
    "md_concat": "cat ./markdown/readme.md ./markdown/server.md ./markdown/app.md > ./README.md",
    "docs_md": "npm run md_server && npm run md_app && npm run md_concat"
  },
  "author": "Lukáš Mačuda <lukas.macuda@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.16.1",
    "@types/node": "^10.14.2",
    "@types/react": "^16.8.8",
    "async": "^2.6.2",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "compression": "^1.7.4",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "cookie-signature": "^1.1.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "http-smart-error": "^2.0.0",
    "md5": "^2.2.1",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "route-parser": "0.0.5",
    "sass-loader": "^7.0.1",
    "smart-error": "^2.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "texting-squirrel": "^0.1.4",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.4000",
    "uniqid": "^5.0.3",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "ignore-styles": "^5.0.1",
    "jsdoc": "^3.5.5",
    "jsdoc-export-default-interop": "^0.3.1",
    "jsdoc-to-markdown": "^4.0.1",
    "request": "^2.88.0",
    "run-script-os-fix": "^1.0.4"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zabkwak/reacting-squirrel.git"
  },
  "bugs": {
    "url": "https://github.com/zabkwak/reacting-squirrel/issues"
  },
  "homepage": "https://github.com/zabkwak/reacting-squirrel#readme",
  "keywords": [
    "react",
    "express",
    "socket.io",
    "framework"
  ],
  "readme": "README.md"
}
