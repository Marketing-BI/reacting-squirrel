{
  "name": "reacting-squirrel",
  "version": "0.14.10",
  "description": "React and Express framework for creating web apps.",
  "main": "index.js",
  "types": "./app/index.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/mocha __tests__/cases/server __tests__/cases/app --recursive --require @babel/register --require ignore-styles --timeout 20000 --exit",
    "test_app_compile": "./node_modules/.bin/babel ./__tests__/dev/src --out-dir ./__tests__/dev/dist && node -r source-map-support/register ./__tests__/dev/dist/index.js",
    "test_app": "npm run build_es6 && npm run test_app_compile",
    "posttest": "rm -rf ./__tests__/app/~rs && rm -rf ./__tests__/public",
    "create_index": "run-script-os",
    "create_index:win32": "echo module.exports = require('./app'); > index.js",
    "create_index:linux:darwin": "echo \"module.exports = require('./app');\" > index.js",
    "build_es6": "./node_modules/.bin/babel ./src --out-dir ./ --copy-files && npm run create_index",
    "clear": "rm -rf ./app && rm -rf ./server && rm -f ./index.js",
    "prepublish": "npm run build_es6",
    "postpublish": "npm run clear",
    "docs_generate": "./node_modules/.bin/typedoc --out docs ./src --includeDeclarations --excludeExternals",
    "docs": "rm -rf ./docs && npm run docs_generate && touch ./docs/.nojekyll",
    "preversion": "git branch %npm_package_version% && git checkout %npm_package_version%"
  },
  "author": "Lukáš Mačuda <lukas.macuda@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-transform-async-to-generator": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/express": "^4.17.2",
    "@types/node": "^10.17.4",
    "@types/react": "^16.9.11",
    "@types/socket.io": "^2.1.4",
    "async": "^2.6.3",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "combine-styles-processor": "^1.3.2",
    "compression": "^1.7.4",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "cookie-signature": "^1.1.0",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "extra-watch-webpack-plugin": "^1.0.3",
    "html-to-react": "^1.4.2",
    "http-smart-error": "^3.0.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "msgpack-lite": "^0.1.26",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.21",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "route-parser": "0.0.5",
    "runtime-type": "^3.1.0",
    "sass-loader": "^8.0.0",
    "smart-error": "^3.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "style-loader": "^1.0.0",
    "texting-squirrel": "^0.1.4",
    "ts-loader": "^6.2.0",
    "typescript": "^3.7.2",
    "uniqid": "^5.1.0",
    "webpack": "^4.41.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/register": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^6.2.2",
    "react-gtm-module": "^2.0.8",
    "request": "^2.88.0",
    "run-script-os-fix": "^1.0.4",
    "source-map-support": "^0.5.16",
    "typedoc": "^0.15.0"
  },
  "engines": {
    "node": ">=8.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zabkwak/reacting-squirrel.git"
  },
  "bugs": {
    "url": "https://github.com/zabkwak/reacting-squirrel/issues"
  },
  "homepage": "https://zabkwak.github.io/reacting-squirrel",
  "keywords": [
    "react",
    "express",
    "socket.io",
    "framework"
  ],
  "readme": "README.md"
}
