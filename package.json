{
	"name": "reacting-squirrel",
	"version": "0.16.9",
	"description": "React and Express framework for creating web apps.",
	"main": "./app/index.js",
	"types": "./app/index.d.ts",
	"scripts": {
		"test": "./node_modules/.bin/mocha __tests__/cases/server __tests__/cases/app --recursive --require @babel/register --require ignore-styles --exit",
		"test:app:compile": "./node_modules/.bin/babel ./__tests__app__/dev/src --out-dir ./__tests__app__/dev/dist && node -r source-map-support/register ./__tests__app__/dev/dist/index.js",
		"test:app": "npm run build_es6 && npm run test:app:compile",
		"posttest": "rm -rf ./__tests__/app/~rs && rm -rf ./__tests__/app/pages && rm -rf ./__tests__/app/components && rm -rf ./__tests__/public",
		"build_es6": "./node_modules/.bin/babel ./src --out-dir ./ --copy-files",
		"clear": "rm -rf ./app && rm -rf ./server && rm -rf ./cli",
		"prepublish": "npm run build_es6",
		"postpublish": "npm run clear",
		"docs:generate": "./node_modules/.bin/typedoc --out docs ./src --includeDeclarations --excludeExternals",
		"docs": "rm -rf ./docs && npm run docs:generate && touch ./docs/.nojekyll",
		"preversion": "git branch %npm_package_version% && git checkout %npm_package_version%",
		"eslint": "./node_modules/.bin/eslint ./src",
		"eslint:server": "./node_modules/.bin/eslint ./src/server",
		"eslint:app": "./node_modules/.bin/eslint ./src/app"
	},
	"author": "Lukáš Mačuda <lukas.macuda@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/plugin-transform-async-to-generator": "^7.8.3",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.9.4",
		"@types/express": "^4.17.4",
		"@types/node": "^10.17.15",
		"@types/react": "^16.9.19",
		"@types/socket.io": "^2.1.4",
		"args-parser": "^1.1.0",
		"async": "^2.6.3",
		"autoprefixer": "^9.7.5",
		"babel-loader": "^8.1.0",
		"combine-styles-processor": "^1.3.2",
		"compression": "^1.7.4",
		"cookie": "^0.4.0",
		"cookie-parser": "^1.4.5",
		"cookie-signature": "^1.1.0",
		"css-loader": "^3.4.2",
		"express": "^4.17.1",
		"extra-watch-webpack-plugin": "^1.0.3",
		"html-to-react": "^1.4.2",
		"http-smart-error": "^3.0.0",
		"loader-utils": "^1.4.0",
		"lodash": "^4.17.15",
		"md5": "^2.2.1",
		"mkdirp": "^0.5.5",
		"msgpack-lite": "^0.1.26",
		"node-sass": "^4.12.0",
		"postcss": "^7.0.27",
		"postcss-loader": "^3.0.0",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"route-parser": "0.0.5",
		"runtime-type": "^3.4.0",
		"sass-loader": "^8.0.2",
		"smart-error": "^3.0.0",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.0",
		"style-loader": "^1.1.3",
		"texting-squirrel": "^1.0.1",
		"ts-loader": "^6.2.2",
		"typescript": "^3.8.3",
		"uniqid": "^5.2.0",
		"universal-cookie": "^4.0.3",
		"webpack": "^4.41.6"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.9.0",
		"@babel/plugin-transform-react-jsx": "^7.9.4",
		"@babel/register": "^7.9.0",
		"@material-ui/core": "^4.9.2",
		"babel-eslint": "^10.1.0",
		"bootstrap": "^4.4.1",
		"chai": "^4.2.0",
		"chai-enzyme": "^1.0.0-beta.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.19.0",
		"ignore-styles": "^5.0.1",
		"jsdom": "^15.2.1",
		"mocha": "^6.2.3",
		"react-bootstrap": "^1.0.0-beta.16",
		"react-gtm-module": "^2.0.8",
		"request": "^2.88.2",
		"run-script-os-fix": "^1.0.4",
		"source-map-support": "^0.5.16",
		"typedoc": "^0.15.8"
	},
	"engines": {
		"node": ">=8.9"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zabkwak/reacting-squirrel.git"
	},
	"bugs": {
		"url": "https://github.com/zabkwak/reacting-squirrel/issues"
	},
	"homepage": "https://zabkwak.github.io/reacting-squirrel",
	"keywords": [
		"react",
		"express",
		"socket.io",
		"framework"
	],
	"readme": "README.md",
	"bin": {
		"rs-start-server": "./cli/server.js"
	}
}
