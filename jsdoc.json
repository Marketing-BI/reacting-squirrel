{
    "plugins": [
        "./node_modules/jsdoc-export-default-interop/dist/index",
        "./node_modules/jsdoc/plugins/markdown"
    ],
    "source": {
        "include": [
            "./src/server",
            "./src/app"
        ]
    },
    "opts": {
        "debug": true,
        "destination": "./docs",
        "readme": "./README.md"
    }
}